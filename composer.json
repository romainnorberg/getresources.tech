{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "algolia/algolia-search-bundle": "3.3.2",
        "easycorp/easyadmin-bundle": "1.17.14",
        "enqueue/enqueue-bundle": "0.8.40",
        "enqueue/redis": "0.8.40",
        "guzzlehttp/guzzle": "6.3.3",
        "league/uri": "5.3.0",
        "predis/predis": "1.1.1",
        "rollbar/rollbar": "1.7.1",
        "sensio/framework-extra-bundle": "5.2.2",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/asset": "^4.0",
        "symfony/cache": "^4.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "doctrine/data-fixtures": "1.3.1",
        "doctrine/doctrine-fixtures-bundle": "3.0.2",
        "hautelook/alice-bundle": "2.3.0",
        "phpstan/phpstan": "0.9.2",
        "phpunit/phpunit": "7.4.4",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.0",
        "symfony/webpack-encore-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "apcu-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "npm install",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C26YDBEESY5TVAVAS68B6EH2",
            "allow-contrib": false
        }
    }
}
